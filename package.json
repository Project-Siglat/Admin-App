{
  "name": "admin-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t siglat-admin-app .",
    "docker:run": "docker run -p 2424:80 --env-file .env siglat-admin-app",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.yml up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f admin-app"
  },
  "description": "SIGLAT Admin App - Emergency Response System Administration",
  "author": "Craftmatrix24",
  "license": "ISC",
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
